<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 当Mapper接口和XML文件关联的时候，命名空间namespace的值需要配置成接口的全限定类名。
        MyBatis内部就是通过这个值将接口和XML关联起来的-->
<mapper namespace="tk.mybatis.mapper.PrivilegeMapper">

    <resultMap id="privilegeMap" type="tk.mybatis.domain.sys_privilege.SysPrivilege">
        <id property="id" column="id"/>
        <result property="privilegeName" column="privilege_name"/>
        <result property="privilegeUrl" column="privilege_url"/>
    </resultMap>

</mapper>